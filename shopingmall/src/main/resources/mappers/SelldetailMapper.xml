<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.spring.my.SelldetailMapper">
 	<!-- ctrl shift y 소문자 x 대문자 -->
 	<insert id="insert">
 		INSERT INTO SELLDETAIL(SCODE,PCODE,SALLCNT,AMOUNT) 
 		VALUES(#{scode},#{pcode},#{sallcnt},#{amount})
 	</insert>
 	
 	
</mapper>